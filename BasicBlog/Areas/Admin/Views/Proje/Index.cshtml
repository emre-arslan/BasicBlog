@model IEnumerable<BasicBlog.projes>
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Shared.cshtml";
}

<p>
    @Html.ActionLink("Create New", "ProjeAdd")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.projeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.thumbnail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.contents)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.language)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.linkedinLink)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.githunLink)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.seoLink)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.productLink)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.projeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.thumbnail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.contents)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.language)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.linkedinLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.githunLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.seoLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.productLink)
            </td>
            <td>
                @Html.ActionLink("Edit", "ProjeUpdate", new { id = item.id }) |
                @*@Html.ActionLink("Details", "Details", new { id=item.id })*@
                @Html.ActionLink("Delete", "ProjeDelete", new { id = item.id })
            </td>
        </tr>
    }

</table>


